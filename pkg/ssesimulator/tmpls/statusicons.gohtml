{{define "status-pending-icon"}}
<img src="/static/svgs/pending.svg" style="height: 24px; stroke-width: 4;">
{{ end }}

{{define "status-completed-icon"}}
<img src="/static/svgs/completed.svg" style="height: 24px; stroke-width: 4;">
{{ end }}


{{define "statusicon"}}
  {{ if eq .Status "pending"}}
    {{ template "status-pending-icon"}}
  {{ else if eq .Status "completed"}}
    {{ template "status-completed-icon"}}
  {{ else }}
    {{ .Status }}
  {{ end }}
{{ end }}
